<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>沖縄県南城市役所</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>nanjocity/service dbfd9c9f55a404091b40efebc6a3a461541d8de0</licence>

  <!-- 翻訳対象のリクエスト情報 （必須）-->
  <request-list>
    <request hostname="www.city.nanjo.okinawa.jp" />
  </request-list>
  
  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>
  
  <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://www\.city\.nanjo\.okinawa\.jp/about-nanjo/introduction/calendar/json-events\.php\?.+</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>
</client-settings>
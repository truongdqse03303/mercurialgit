<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社平成エンタープライズ</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>vipliner/service 081e16bcba79978ef97448005e98daae2179eb03</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="vipliner.biz" />
    <request hostname="dev.vipliner.biz" />
  </request-list>

  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.*</url>
  </url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社ヤマゼンコミュニケイションズ</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>tochinavi/service 4301209bfbb6e3df2af97d5091d66127f536468f</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.tochinavi.net" />
  </request-list>
  
    <!-- 免責ヘッダーを絶対座標で指定 -->
  <extension-param>
    <param-name>site.translate.header.style</param-name>
    <param-value>style:#site-translate-header-wrapper{width:auto !important;z-index:10000;}</param-value>
  </extension-param>
  
    <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://www\.tochinavi\.net/common/css/webfonts\.css</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>BINARY</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社西武プロパティーズ</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>tgt-kioicho/service fb440fc66eecd6b1bb4cd9ff36c6c8abf9c7310e</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.tgt-kioicho.jp" />
  </request-list>
  
    <!-- 免責ヘッダーを絶対座標で指定 -->
  <extension-param>
    <param-name>site.translate.header.style</param-name>
    <param-value>style:#site-translate-header-wrapper{width:auto !important;}</param-value>
  </extension-param>
  
    <!-- 翻訳対象外 -->
  <url-list type="ignore">
    <url>http://www\.tgt-kioicho\.jp/make/.*</url>
    <url>http://www\.tgt-kioicho\.jp/residence/.*</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社プランタン銀座</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>printemps-ginza/service b5a2ac9c841e958f72aa9ccd45089c2c6e3a5a86</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.printemps-ginza.co.jp" />
  </request-list>
  
  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>学校法人 東放学園</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>tohogakuen/service 76a4ccb29be0a90fefdca6b434a99ecd0e7de12e</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.tohogakuen.ac.jp" ssl="true" />
  </request-list>

  <extension-param>
	<param-name>site.translate.header.style</param-name>
	<param-value>client-settings.css</param-value>
  </extension-param>

  <!-- Microsoft Translatorロゴを表示する設定ID -->
  <extension-param>
   <param-name>mst.logo.show</param-name>
   <param-value>v,id</param-value>
  </extension-param>


  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

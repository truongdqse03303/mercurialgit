<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>西鉄旅行株式会社</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>kyushu-note/service bc58253a4589a98e110892e7fc90fd6837309c42</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.kyushu-note.jp" />
  </request-list>
  
  <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
  </extension-param>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

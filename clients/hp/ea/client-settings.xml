<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>渋谷観光協会</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>shibuyakukanko/service 5efd25b6860c56f1a46c28113be7552586392cea</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="shibuyakukanko.jp" />
  </request-list>

  
  <!-- キャッシュされないPDFをダウンロードしない -->
  <ignore-url-list>
    <url>.+/_images/mappdf/.+\.pdf</url>
  </ignore-url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>
</client-settings>

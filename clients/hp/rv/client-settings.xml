<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>二子玉川ライズ</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>rise-nikotama/service 16856969c76358b7ade3d6b5f354c50341e7664c</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="sc.rise.sc" />
    <request hostname="www.rise.sc" />
  </request-list>
  
   <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://sc.rise.sc/shop/category_json/.*</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

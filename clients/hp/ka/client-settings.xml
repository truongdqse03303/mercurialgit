<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>すまいりんぐ.com</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>smilring/service 3e59e9b4b5ff4eed53528f1be41e3f0c6a5350c5</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.smilring.com" />
  </request-list>

  <!-- HTML断片翻訳対象外のURL -->
  <url-list type="ignore-fragment">
    <url>http://www\.smilring\.com/shop/.*</url>
  </url-list>
  
    <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://www\.smilring\.com/application/controllers/SearchInput\.php</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

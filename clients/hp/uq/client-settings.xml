<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>南海マネジメントサービス株式会社</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>nambaparks/service 6d309286b73e6253dc9f315c42313e52a297897c</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.nambaparks.com" />
  </request-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- 免責ヘッダーは表示しない -->
  <extension-param>
    <param-name>show.translate.header</param-name>
    <param-value>false</param-value>
  </extension-param>
</client-settings>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

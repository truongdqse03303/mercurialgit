<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>南三陸町役場</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>miyagi-minamisanriku/service eee410d79c2e9840f19f2fb56e7f8623dd86bdc1</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.town.minamisanriku.miyagi.jp" />
  </request-list>
  
  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>
  
  <!-- HTML断片翻訳のスプリクトタグの挿入しないURLパターン -->
  <extension-param>
    <param-name>fragment.translate.tag.unset.url.pattern</param-name>
    <param-value>http://www\.town\.minamisanriku\.miyagi\.jp/.+.php</param-value>
  </extension-param>
  
  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

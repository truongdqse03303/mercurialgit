<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>阿蘇観光協会</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>asocity-kanko/service fabb0f6fb7043f0d1c06b067c14cad13d2d9f59d</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.asocity-kanko.jp" />
  </request-list>
  
  <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
  </extension-param>
  
  <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://www\.asocity-kanko\.jp/onsen/feedcollection\.php</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

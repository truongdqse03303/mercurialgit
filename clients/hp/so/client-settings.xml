<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>恩納マリンビューパレス</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>onna-marineviewpalace/service 2867d38bdad26eb730a07b094b9b14917c360f4b</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.onna-marineviewpalace.jp" />
  </request-list>

  <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
  </extension-param>
  
  <!-- 免責ヘッダーを絶対座標で指定 -->
  <extension-param>
    <param-name>site.translate.header.style</param-name>
    <param-value>style:#site-translate-header-wrapper{width:auto !important;}</param-value>
  </extension-param>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>杖立観光ホテルひぜんや</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>hizenya/service d45909a64de3b17fe1c122ef2ab4553a6cfcea65</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.hizenya.co.jp" />
  </request-list>
  
  <!-- 使用する設定IDの一覧 -->
  <setting-id>mb-*</setting-id>

  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

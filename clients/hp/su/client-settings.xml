<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社アートスポーツ</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>art-sports/service 599141b0c97a3fc5dc6698438d46c32110b135c9</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="art-sports.jp" />
  </request-list>
  
  <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
  </extension-param>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

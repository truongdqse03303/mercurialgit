<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社ブーマー</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>warpweb/service 019ea815ec0cee28750ff39d04e513012728afa3</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.warpweb.jp" />
  </request-list>
  
  <!-- 使用する設定IDの一覧 -->
  <setting-id>mb-*</setting-id>

  <!-- HTML断片翻訳しないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社エスティーエートラベル</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>statravel/service 5192f291d02d7c69f5c5a7e16c45f3457c32f5aa</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.statravel.co.jp" />
  </request-list>
  
  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>
</client-settings>

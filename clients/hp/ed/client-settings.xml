<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>阿蘇ジオパーク</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>aso-geopark/service 6f8e11fcb9ed73ca3f639476e90a2f4c202d576a</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.aso-geopark.jp" />
  </request-list>
  
  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <ignore-fragment-translate-list>
    <url>.+</url>
  </ignore-fragment-translate-list>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

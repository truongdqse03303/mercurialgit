<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>プリンスホテル:中国語用</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>princehotels/princehotels01 3b0525cd4f9e9eccb0bb0fbe27c48555e2a1bd53</licence>

  <!-- 翻訳対象のリクエスト情報 （必須）-->
  <request-list>
    <request hostname="www.princehotels.co.jp" />
  </request-list>
  
  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://www\.princehotels\.co\.jp/(?!karuizawa-west|shinjuku|karuizawa-east|the_prince_karuizawa|asama|common/(?:css|js|imgs)).*</url>
  </ignore-url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>false</use-cache>
  
  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>ルートインジャパン株式会社</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>route-inn/service 3aaf76ab1bb088b923bec99fe891b49364c72d41</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.route-inn.co.jp" />
  </request-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>
  
    <!-- 翻訳対象外 -->
  <url-list type="ignore">
    <url>http://www\.route-inn\.co\.jp/ponta/</url>
  </url-list>
  
      <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
 </extension-param>
  
  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

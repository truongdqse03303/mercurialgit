<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>全国老人福祉施設協議会</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>roushikyo/service 56657f55a32425df8576b8e50fdcd9047034da46</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.roushikyo.or.jp" path="/contents/" ssl="true" />
  </request-list>

  <extension-param>
  <param-name>site.translate.header.style</param-name>
  <param-value>client-settings.css</param-value>
  </extension-param>

  <!-- Microsoft Translatorロゴを表示する設定ID -->
  <extension-param>
   <param-name>mst.logo.show</param-name>
   <param-value>v,id</param-value>
  </extension-param>


  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

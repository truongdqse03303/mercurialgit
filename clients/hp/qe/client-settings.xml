<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>イオンモール木更津</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>kisarazu-aeonmall/service e767fb9f7a47b4acab8e6ec622fc18ba7e23a4d0</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="kisarazu-aeonmall.com" />
  </request-list>
  
  <!-- デフォルトでHTML断片翻訳を行うかどうか -->
  <extension-param>
   <param-name>default.fragment.translate</param-name>
   <param-value>false</param-value>
  </extension-param>
  
  <!-- 免責ヘッダーを絶対座標で指定 -->
  <extension-param>
    <param-name>site.translate.header.style</param-name>
    <param-value>style:#site-translate-header-wrapper{width:auto !important;}</param-value>
  </extension-param>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

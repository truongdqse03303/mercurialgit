<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>株式会社アイセン</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>aisen/service cc059cb5208b5b916d517b9f513f73b5dd1d6d84</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.aisen.co.jp" />
  </request-list>
  
  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://www\.aisen\.co\.jp/ebook/.+</url>
  </ignore-url-list>
  
  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <ignore-fragment-translate-list>
    <url>http://www\.aisen\.co\.jp/products/.+</url>
  </ignore-fragment-translate-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>ピーアンドピー</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>pandp/service 6f7a7dce910e62f081701368bf7931047ac1d074</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.pandp.net" />
  </request-list>
  
  <!-- 使用する設定IDの一覧 -->
  <setting-id>mb-*</setting-id>

  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

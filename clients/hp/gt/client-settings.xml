<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>中央共同募金会</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>akaihane/service 411eea8dc390d1e1d598def47c102134dc5d05f8</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="akaihane.or.jp" />
    <request hostname="www.akaihane.or.jp" />
    <request hostname="hanett.akaihane.or.jp" path="/hanett" />
  </request-list>
  
  <!-- HTML断片翻訳いないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>http://(www\.)?akaihane\.or\.jp/.*</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

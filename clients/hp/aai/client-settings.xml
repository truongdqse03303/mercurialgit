  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>aeon-hokkaido/service a5db97df6e02ce5489014256e3c43fb4adc9f015</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.aeon.jp">
      <path-list>
        <path value="/sc/asahikawanishi/" />
        <path value="/sc/sapporohassamu/" />
        <path value="/sc/sapporonaebo/" />
        <path value="/sc/sapporosoen/" />
        <path value="/sc/sapporomotomachi/" />
        <path value="/sc/sapporonishioka/" />

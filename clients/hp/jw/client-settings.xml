<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>芭蕉市事業協同組合</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>bashofu/service f0c5463098f6d71a7b0a52010a2090027fc0fc36</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="bashofu.jp" />
  </request-list>

  <!-- HTML断片翻訳は実行しない -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>川崎市観光協会</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>k-kankou/service 73a1cd0fd9f841582d7d98d6b83318b1cadf6213</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.k-kankou.jp" />
  </request-list>

  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <ignore-fragment-translate-list>
    <url>.+</url>
  </ignore-fragment-translate-list>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

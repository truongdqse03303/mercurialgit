<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>横浜港埠頭公社</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>ypdc/ypdc01 5aaf0bf04bfd3020d6c7fbf6348d4512c64f0841</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.yokohamaport.co.jp" />
    <request hostname="www.ypdc.or.jp" />
  </request-list>
  
  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://www\.yokohamaport\.co\.jp/(?:tender/entry|guidance/reserve|inquiry)/.*</url>
    <url>http://www\.ypdc\.or\.jp/(?:tender/entry|guidance/reserve|inquiry)/.*</url>
  </ignore-url-list>

  <!-- サイトで使用される文字セット -->
  <charset>UTF-8</charset>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>イオンモール福岡</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>fukuoka-lucle/service 365b64674ec4782282b9c690b929216b8b3845aa</licence>
    
  <!-- クライアントが使用できるドメインの一覧 -->
  <request-list>
    <request hostname="fukuoka-aeonmall.com" />
  </request-list>
  
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>http://fukuoka-aeonmall\.com/js/weather/getWeather\.php\?.+</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>
  
  <!-- 翻訳しないURLの一覧 -->
  <url-list type="ignore">
    <url>http://fukuoka-aeonmall.com/floorguide-ch/</url>
    <url>http://fukuoka-aeonmall.com/floorguide-en/</url>
    <url>http://fukuoka-aeonmall.com/floorguide-kr/</url>
  </url-list>
 
  <!-- サイトで使用される文字セット -->
  <charset>Shift_JIS</charset>
</client-settings>
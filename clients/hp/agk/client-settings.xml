<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>えひめさんさん物語</name>

  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>ehimesansan/service 1a7fb05f768c479cfb1266a589435d9b5ba6a5e2</licence>

  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="ehimesansan.jp" ssl="true" ssl_verifypeer="false" />
  </request-list>

  <extension-param>
  <param-name>site.translate.header.style</param-name>
  <param-value>client-settings.css</param-value>
  </extension-param>

  <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>(http|https)://ehimesansan\.jp/eventsearch/.*</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>BINARY</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

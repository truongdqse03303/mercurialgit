<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>マーケットネットワーク</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>sekaiisan-nikko/service a64529faee032e787ddfc411c06021ec2894fff0</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="www.sekaiisan-nikko.jp" />
  </request-list>
  
  <!-- 使用する設定IDの一覧 -->
  <setting-id>mb-*</setting-id>

  <!-- HTML断片翻訳を行わないURLの一覧 -->
  <url-list type="ignore-fragment">
    <url>.+</url>
  </url-list>

  <!-- サイトで使用される文字セット -->
  <!--charset>UTF-8</charset-->
</client-settings>

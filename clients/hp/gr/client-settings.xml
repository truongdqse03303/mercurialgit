<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>TOKYO MX</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>mxtv/service 5b3b95396ba09f253419c24565e1363c9b5d7952</licence>
  
  <!-- 翻訳対象となるリクエストの一覧 -->
  <request-list>
    <request hostname="s.mxtv.jp" />
  </request-list>

  <!-- 断片翻訳スクリプトタグを挿入しない場合に以下を記載 -->
  <extension-param>
    <param-name>fragment.translate.tag.insert.setting</param-name>
    <param-value>unset</param-value>
  </extension-param>

  <!-- 使用する設定IDの一覧 -->
  <setting-id>mb-*</setting-id>
</client-settings>

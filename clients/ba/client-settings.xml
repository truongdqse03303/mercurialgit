<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>丸井今井</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>marui-imai/service 0370ecb92823a6f9e125d765cc9eebef06f34539</licence>

  <!-- 翻訳対象となるドメイン(URL)の一覧 -->
  <domain-list>
    <domain>http://www.marui-imai\.jp/</domain>
  </domain-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>
  
  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>@Homepageモニター用ページ</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>cross/athome d6ce0f1392b1f2d7304e1cd99326b4beb7b7f4b4</licence>
  
  <!-- クライアントが使用できるドメインの一覧 -->
  <!-- 基本的にトップレベルドメインは省略できない -->
  <!-- すべてのドメインに対して翻訳を行いたい場合は、"https?://[^/]+"と指定すること -->
  <!-- "https?://"は不要だが、リライトすルールが正しく適用されない場合が多い -->
  <domain-list>
    <domain>http://www.crosslanguage.co.jp/wormly/</domain>
  </domain-list>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>有限会社レイヤード</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>layered/service 3ecc2ac8baa2b35b5fa79345ea220f24f6e91649</licence>
  
  <!-- クライアントが使用できるドメインの一覧 -->
  <domain-list>
    <domain>http://(?:www\.)?layered\.me/</domain>
  </domain-list>
  
  <!-- 拡張パラメータ（任意）。複数指定可能 -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

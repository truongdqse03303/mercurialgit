<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>瀬戸内市役所</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>setouchi/setouchi01 769068d0e66c5c4421f5ba32ef4b192eee92dd4f</licence>

  <!-- クライアントが使用できるドメイン （必須）-->
  <domain-list>
    <domain>http://www\.city\.setouchi\.lg\.jp/</domain>
  </domain-list>
  
  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://www\.city\.setouchi\.lg\.jp/i/.*</url>
    <url>http://www\.city\.setouchi\.lg\.jp/(~osa-token/)?english/.*</url>
  </ignore-url-list>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>clientcc</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>cross/athome d6ce0f1392b1f2d7304e1cd99326b4beb7b7f4b4</licence>
  
  <!-- コンテンツキャッシュを使用するかどうかの設定 -->
  <!-- 省略可能。デフォルト値は、false -->  
  <use-cache>false</use-cache>
  
  <!-- クライアントが使用できるドメインの一覧 -->
  <domain-list>
    <domain>http://[^/]+/</domain>
  </domain-list>

  <!-- 拡張パラメータ（任意）。複数指定可能 -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

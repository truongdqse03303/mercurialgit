<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>ＭＣクラウド</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>admin f4fee6298151b3be9bb0fa4b8dd61f214e6320ec</licence>

  <!-- クライアントが使用できるドメイン （必須）-->
  <domain-list>
    <domain>http://(?:www\.)?mc-cloud.jp/</domain>
  </domain-list>

  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

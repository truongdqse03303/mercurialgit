<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>プリンスホテル</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>princehotels/princehotels01 3b0525cd4f9e9eccb0bb0fbe27c48555e2a1bd53</licence>

  <!-- 翻訳対象となるドメイン(URL)の一覧 -->
  <domain-list>
    <domain>http://.+\.princehotels\.co\.jp</domain>
  </domain-list>

  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://princehotels\.com(?:$|/$|.+)</url>
  </ignore-url-list>
  
  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>
  
  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>プリンスホテル</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>princehotels/princehotels01 3b0525cd4f9e9eccb0bb0fbe27c48555e2a1bd53</licence>

  <!-- 翻訳対象となるドメイン(URL)の一覧 -->
  <domain-list>
    <domain>http://.+\.princehotels\.co\.jp</domain>
  </domain-list>

  <!-- 翻訳しないURLの一覧 -->
  <ignore-url-list>
    <url>http://princehotels\.com(?:$|/$|.+)</url>
  </ignore-url-list>
  
  <!-- コンテンツキャッシュ使用有無の設定-->
  <use-cache>true</use-cache>
  
  <!-- サイトで使用される文字セット -->
  <charset>utf-8</charset>

  <!-- URLとリソース種類のマッピングの設定 -->
  <resource-handler>
    <type>jp.co.crosslanguage.translate.url.request.process.resource.handler.SpecifiedTypeResourceCreateHandler</type>
    <param>
      <param-name>resource.url.pattern</param-name>
      <param-value>^http://golf2\.princehotels\.co\.jp/topics/topics\.asp\?.+</param-value>
    </param>
    <param>
      <param-name>resource.type.name</param-name>
      <param-value>JAVASCRIPT</param-value>
    </param>
    <priority>4</priority>
  </resource-handler>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

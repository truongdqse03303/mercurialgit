<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>刈谷市</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>kariya/kariya01 f54737ded70ca8deecd602f52f3bf440c4a13e1f</licence>

  <!-- クライアントが使用できるドメインの一覧 -->
  <domain-list>
    <domain>http://www\.city\.kariya\.lg\.jp</domain>
    <!-- Googleサイト内検索 -->
    <domain>http://www\.google\.co\.jp/search(\?.*&amp;?domains=city\.kariya\.lg\.jp.*)?</domain>
  </domain-list>

  <!-- サイトで使用される文字セット -->
  <charset>Shift_JIS</charset>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>

<?xml version="1.0" encoding="UTF-8"?>
<client-settings>
  <!-- クライアント名（任意） -->
  <name>武蔵野市役所</name>
  
  <!-- Translation Serverのライセンスキー（必須） -->
  <licence>musashino/musashino01 0f55d68caebf845da6312c3838412b942f11899d</licence>

  <!-- クライアントが使用できるドメイン （必須）-->
  <domain-list>
    <domain>http://www\.city\.musashino\.lg\.jp</domain>
  </domain-list>

  <!-- 設定IDのためにドメインを使用する -->
  <extension-param>
    <param-name>use.domain.setting</param-name>
    <param-value>true</param-value>
  </extension-param>
</client-settings>
